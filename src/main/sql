create table timer
(
    nome    varchar(20)    not null,
    break   int default 5  null,
    session int default 25 null,
    userId  int            not null
);

create table appunti
(
    titolo   varchar(50)   not null,
    testo    varchar(8000) not null,
    materia  varchar(30)   not null,
    creatore int           not null
);